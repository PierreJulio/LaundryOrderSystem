{
  "version": 2,
  "framework": null,
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist/laundry-order-app/browser",
  "installCommand": "npm install",
  "env": {
    "NODE_VERSION": "18",
    "NPM_CONFIG_PRODUCTION": "false"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
